{
	"env": {
	  "es6": true,
	  "jest": true
	},
	"extends": "airbnb",
	"parser": "babel-eslint",
	"plugins": [
	  "react",
	  "detox",
	  "jest"
	],
	"globals": {
		"fetch": false
	},
	"rules": {
	  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
	  "no-use-before-define": 0,
	  "no-underscore-dangle": 0,
	  "object-curly-newline": [2, { "consistent": true, "minProperties": 6 }],
	  "react/sort-comp": [1, {"order": ["lifecycle", "render", "static-methods", "everything-else"]}],
	  "no-unused-expressions": [2, { "allowTernary": true }],
	  "max-len": [2, 150],
	  "import/prefer-default-export": 0,
	  "import/no-cycle": 0,
	  "no-plusplus": 0,
	  "no-shadow": ["error", { "builtinGlobals": false, "hoist": "all", "allow": [] }]
	},
	"settings": {
	  "import/resolver": {
		"babel-module": {
			"extensions": [".js", ".jsx"]
		  }
	  }
	},
	"parserOptions": {
	  "ecmaFeatures": { "legacyDecorators": true }
	}
  }
  